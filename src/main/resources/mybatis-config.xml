<?xml version="1.0" encoding="UTF-8"?>
<!-- config 사용 하기위해선 mapper->configuration, config로 고치기 -->
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<!-- 별명을 간단하게 만들어서 myBatis mapper에서 그 값을 간단히 사용할 수 있게 끔 함 -->
<configuration>

	<typeAliases>
		<!-- 저 type명을 별명으로 간단히 사용하겠다. -->
		<typeAlias type="com.hiworld.client.vo.ClientVO" alias="clientVO"/>
		<typeAlias type="com.hiworld.client.vo.sessionVO" alias="sessionVO"/>
		<typeAlias type="com.hiworld.article.vo.ArticleVO" alias="articleVO"/>
		<typeAlias type="com.hiworld.client.vo.BoardVO" alias="boardVO"/>
		<typeAlias type="com.hiworld.client.vo.BoardReplyVO" alias="boardReplyVO"/>
	</typeAliases>

</configuration>

